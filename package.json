{
  "name": "@x-govuk/govuk-prototype-filters",
  "version": "1.0.0",
  "description": "Prototype with data using helpful template filters",
  "keywords": [
    "prototyping",
    "government",
    "govuk-prototype-kit"
  ],
  "homepage": "https://x-govuk.github.io/govuk-prototype-filters/",
  "bugs": {
    "url": "https://github.com/x-govuk/govuk-prototype-filters/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "govuk-prototype-kit.config.json"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/x-govuk/govuk-prototype-filters.git"
  },
  "scripts": {
    "build": "eleventy --quiet",
    "start": "eleventy --serve --quiet",
    "lint": "eslint '**/*.js'",
    "test": "ava",
    "coverage": "c8 ava"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "govuk-markdown": "^0.4.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "marked": "^5.0.0",
    "marked-smartypants": "^1.0.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@x-govuk/govuk-eleventy-plugin": "^5.0.0",
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jsdoc": "^44.2.0"
  },
  "peerDependencies": {
    "govuk-prototype-kit": "^13.0.0"
  },
  "engines": {
    "node": "^18"
  },
  "c8": {
    "reporter": [
      "text",
      "lcovonly"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:jsdoc/recommended",
      "standard"
    ],
    "rules": {
      "jsdoc/tag-lines": 0
    }
  },
  "eslintIgnore": [
    "_site"
  ]
}
